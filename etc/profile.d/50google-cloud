# =============================================================================
# Google Cloud SDK
# =============================================================================

ME_GCLOUD_SDK_VERSIONS=(276.0.0)
ME_GCLOUD_SDK_DIR=$ME_PLATFORM_TOOL_DIR/google-cloud-sdk

export USE_GKE_GCLOUD_AUTH_PLUGIN=True

ME_PATH+=:$ME_GCLOUD_SDK_DIR/bin
if [ -f $ME_GCLOUD_SDK_DIR/completion.zsh.inc ]; then
  . $ME_GCLOUD_SDK_DIR/completion.zsh.inc ]
fi

