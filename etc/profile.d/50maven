ME_MAVEN_VERSIONS=(3.6.0 3.5.4 3.3.9 3.2.5 3.1.1 3.0.5 2.2.1)

ME_PATH+=:$ME_TOOL_DIR/opt/maven/bin

function m() {
  cmd="./mvnw $@"
  echo "% $cmd"
  eval $cmd
}

