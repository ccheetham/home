dirs="/usr/bin /bin /usr/sbin /sbin"
for d in $(echo $dirs) ; do
    remove_path $d
    append_path $d
done

dirs="/usr/local/sbin /usr/local/bin"
for d in $(echo $dirs) ; do
    prepend_path $d
done

export MANPATH=/usr/local/share/man:/usr/share/man

os_specific=$(dirname $0)/$ME_OSNAME
[[ -f $os_specific ]] && source $os_specific

# vim: ft=zsh
