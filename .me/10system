dirs="/usr/bin /bin /usr/sbin /sbin"
for d in $(echo $dirs) ; do
    remove_path $d
    append_path $d
done

dirs="/usr/local/bin /usr/local/bin"
for d in $(echo $dirs) ; do
    prepend_path $d
done

if [[ -z $MANPATH ]] ; then
    MANPATH=/usr/local/share/man
else
    MANPATH=/usr/local/share/man:$MANPATH
fi

os_specific=$(dirname $0)/$ME_OSNAME
[[ -f $os_specific ]] && source $os_specific

# vim: ft=zsh
