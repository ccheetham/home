# =============================================================================
# file management
# =============================================================================

# -----------------------------------------------------------------------------
# tree: http://mama.indstate.edu/users/ice/tree/
# -----------------------------------------------------------------------------
checkexe tree && alias tree='tree -C -I \*.pyc'

# -----------------------------------------------------------------------------
# ranger: http://ranger.nongnu.org/
# -----------------------------------------------------------------------------
checkexe ranger && alias rr='ranger'

